<main class="content">
	<div class="content-header ui-content-header">
		<div class="container">
			<div class="row">
				<div class="col-lg-10 col-lg-push-1 col-sm-10 col-sm-push-1">
					<h1 class="content-heading">학과별 강의</h1>
				</div>
			</div>
		</div>
	</div>
	<div class="container">
		<section class="content-inner margin-top-no">
            <br>
            <br>
            <br>
            <br>
            <%= form_for(:session, url: login_path) do |f| %>
                <%= f.text_field :name, :placeholder => "닉네임" %>
                <%= f.password_field :password, :placeholder => "비밀번호" %>
                <%= f.submit "로그인 하기", class: "btn waves-effect waves-light" %>
                <%= link_to "가입하기", '/users/new', class: "btn waves-effect waves-light" %>
            <% end %>
        </section>
    </div>
</main>
